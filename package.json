{
  "name": "kanjidate",
  "version": "1.3.3",
  "description": "a small library that formats dates in Japanese",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/hangilc/kanjidate.git"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup index.ts --format cjs,esm --dts --clean",
    "test": "mocha --reporter spec",
    "tsc": "tsc"
  },
  "keywords": [
    "date"
  ],
  "author": "Hangil Chang",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.3.6",
    "mocha": "^10.0.0",
    "tsup": "^6.2.3",
    "typescript": "^4.8.4"
  }
}
